// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model AgeFactor {
  id     String @id @default(uuid())
  age    Int
  factor Float
}

model Occupation {
  id       String     @id @default(uuid())
  code     String
  name     String
  active   Boolean
  factor   Float
  Isurance Isurance[]

  @@map("occupation")
}

model Isurance {
  id             String       @id @default(uuid())
  age            Int
  occupationCode String
  Occupation     Occupation[]
  capital        Int
  coverages      String[]

  @@map("Insurance")
}
